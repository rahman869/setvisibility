        DecimalFormat persen = (DecimalFormat) NumberFormat.getInstance(Locale.US);
        persen.applyPattern("#,## %");

        DecimalFormat persen1 = (DecimalFormat) NumberFormat.getInstance(Locale.US);
        persen1.applyPattern("#,# %");


